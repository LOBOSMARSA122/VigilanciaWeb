        var mm = String(today.getMonth() + 1).padStart(2, '0'); //January is 0!
        var yyyy = today.getFullYear();

        today = yyyy + '-' + mm + '-' + dd;
        let error = false;
        if (nombre.val() == "") {
            nombre.addClass("border-danger").removeClass("border-success");
            error = true;
        } else {
            nombre.addClass("border-success").removeClass("border-danger");
        }
        if (apepat.val() == "") {
