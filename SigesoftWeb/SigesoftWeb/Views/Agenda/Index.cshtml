        if (genero.val() == "-1") {
            genero.addClass("border-danger").removeClass("border-success");
            error = true;
        } else {
            genero.addClass("border-success").removeClass("border-danger");
        }
        if (error) {
            alertafixed({
                class: 'validacion',
                type: 'warning',
                title: 'VALIDACIÓN',
