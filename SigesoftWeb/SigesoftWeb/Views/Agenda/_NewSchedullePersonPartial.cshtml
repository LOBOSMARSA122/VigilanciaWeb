                                        <option selected value="-1">--Seleccionar--</option>
                                        @{
                                            foreach (var item in ViewBag.Users)
                                            {
                                                <option value="@item.Id">@item.Value</option>
                                            }
                                        }
                                    </select>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
            <div class="modal-footer row w-100 mx-0 py-2 px-4 justify-content-between" id="footer-new-agenda">
                <button type="button" class="btn btn-secondary" data-dismiss="modal"><i class="fas fa-times mr-1"></i>Close</button>
                <button type="button" class="btn btn-primary" id="btn-guardar-agenda"><i class="far fa-calendar-alt mr-1"></i>Agendar</button>
            </div>
        </div>
    </div>
</div>

